{
  "name": "autonetworks-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:e2e": "npx playwright test",
    "test:e2e:dev": "npx playwright test --config=playwright.config.dev.ts",
    "lint": "next lint",
    "lint:fix": "prettier --write --ignore-path .gitignore '{src,api}/**/*.{js,jsx,ts,tsx,json}' && eslint --fix --ext .{js,jsx,ts,tsx,json} .",
    "generate:api": "node api/generate.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "record:screenshot": "yarn build-storybook && storycap --serverCmd \"npx http-server storybook-static -p 6006\" http://localhost:6006",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/noto-sans": "^4.5.11",
    "@fontsource/noto-sans-jp": "^4.5.12",
    "@hookform/resolvers": "^3.1.1",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@next/font": "13.2.0",
    "axios": "^1.3.4",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "next": "13.2.0",
    "plotly.js": "^2.21.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.0",
    "react-plotly.js": "^2.6.0",
    "recoil": "^0.7.6",
    "recoil-persist": "^4.2.0",
    "universal-cookie": "^4.0.4",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/blocks": "^7.0.5",
    "@storybook/nextjs": "^7.0.5",
    "@storybook/react": "^7.0.5",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/testing-react": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.1",
    "@types/plotly.js": "^2.12.18",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-plotly.js": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "camelcase": "^7.0.1",
    "eslint": "8.34.0",
    "eslint-config-next": "13.2.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "hygen": "^6.2.11",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "msw": "^1.2.3",
    "msw-storybook-addon": "^1.8.0",
    "prettier": "^3.0.0",
    "storybook": "^7.0.5",
    "storycap": "^4.2.0",
    "swagger-typescript-api": "^12.0.4",
    "typescript": "4.9.5"
  },
